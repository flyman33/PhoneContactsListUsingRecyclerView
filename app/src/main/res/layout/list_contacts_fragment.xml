        <TextView
            android:id="@+id/contact5_last_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textSize="20sp"
            android:layout_marginEnd="25dp"
            android:textStyle="bold"
            android:text="@string/contact5_lastname"/>

        <TextView
            android:id="@+id/contact5_phone_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textSize="20sp"
            android:text="@string/contact5_phone_number"/>

    </LinearLayout>



</LinearLayout>